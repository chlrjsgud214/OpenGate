<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias alias="notice" type="com.og.opengate.model.Notice" />
		<typeAlias alias="member" type="com.og.opengate.model.Member"/>
		<typeAlias alias="product" type="com.og.opengate.model.Product"/>
		<typeAlias alias="reviewboard" type="com.og.opengate.model.ReviewBoard" />
		<typeAlias alias="noticerbd" type="com.og.opengate.model.NoticeReplyBoard"/>
	</typeAliases>
	<mappers>
		<mapper resource="notice.xml" />
		<mapper resource="member.xml" />
		<mapper resource="Product.xml" />
		<mapper resource="reviewboard.xml" />
		<mapper resource="noticerbd.xml" />
	</mappers>
</configuration>
